{
  "version": 2,
  "builds": [
    {
      "src": "Backend/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:css|js|html|ico|png|jpg|jpeg|gif|svg))$",
      "dest": "Frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Backend/app/main.py"
    }
  ]
}
